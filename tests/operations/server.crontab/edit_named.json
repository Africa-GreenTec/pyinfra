{
    "args": ["this_is_another_command"],
    "kwargs": {
        "month": 1,
        "name": "this_is_a_command"
    },
    "facts": {
        "crontab": {
            "this_is_a_command": {
                "minute": "*",
                "hour": "*",
                "month": "*",
                "day_of_week": "*",
                "day_of_month": "*",
                "comments": [
                    "# pyinfra-name=this_is_a_command"
                ]
            }
        }
    },
    "commands": [
        "crontab -l  > _tempfile_",
        "sed -i='' 's/.*this_is_a_command.*/* * * 1 * this_is_another_command/' _tempfile_",
        "crontab  _tempfile_"
    ]
}
