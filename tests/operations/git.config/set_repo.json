{
    "args": ["key", "value"],
    "kwargs": {
        "repo": "/my/repo"
    },
    "facts": {
        "git_config": {
            "/my/repo": {
                "another_key": "value"
            }
        },
        "directory": {
            "/my/repo/.git": true
        }
    },
    "commands": [
        "cd /my/repo && git config --local key \"value\""
    ]
}
