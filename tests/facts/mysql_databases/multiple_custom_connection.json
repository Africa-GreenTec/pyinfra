{
    "arg": ["myuser", "mypassword", "myhost", "myport"],
    "command": [
        "which mysql > /dev/null && (mysql -u\"myuser\" -p\"mypassword\" -hmyhost -Pmyport -Be 'SELECT * FROM information_schema.SCHEMATA') || true",
        "which mysql > /dev/null && (mysql -u\"myuser\" *** -hmyhost -Pmyport -Be 'SELECT * FROM information_schema.SCHEMATA') || true"
    ],
    "output": [
        "CATALOG_NAME\tSCHEMA_NAME\tDEFAULT_CHARACTER_SET_NAME\tDEFAULT_COLLATION_NAME\tSQL_PATH",
        "def\tinformation_schema\tutf8\tutf8_general_ci\tNULL"
    ],
    "fact": {
        "information_schema": {
            "character_set": "utf8",
            "collation_name": "utf8_general_ci"
        }
    }
}
